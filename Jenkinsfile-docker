pipeline {
  agent {
      node {
          label 'master'
      }
  }
  stages {
    stage("Test"){
      steps {
        script {
          def img = docker.image("ubuntu")
          img.inside(){
            sh """bash
            cat /etc/os-release
            whoami
            hostname
            """
          }
        }
      }
    }
  }
}
