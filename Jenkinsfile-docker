pipeline {
  agent {
      node {
          label 'master'
      }
  }
  stages {
    stage("Test"){
      steps {
        script {
          def img = docker.image("ubuntu")
          img.inside(){
            sh 'cat /etc/os-release'
            sh 'whoami'
            sh 'hostname'
          }
        }
      }
    }
  }
}
